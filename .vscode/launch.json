    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "app.py",
            "console": "integratedTerminal",
            "env": {
                "LOG_LEVEL": "DEBUG",
                "FEED_URL": "https://rss-feed-url.com/feed-with-1251/",
                "FEED_ENCODING": "windows-1251",
                "TRANSFORM_RULES": "[{\"field\":\"link\",\"regex\":\"^(https?://.*)$\",\"replacement\":\"https://proxy.example.com/?url=$1\"}]"
            }
        }
    ]
}